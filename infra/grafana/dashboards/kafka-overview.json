{
  "title": "Aurora Kafka Overview",
  "panels": [
    {
      "type": "graph",
      "title": "Mensagens/s (pilares)",
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_messagesin_total{topic=\"pilares\"}[1m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Bytes In/Out",
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesin_total{topic=\"pilares\"}[1m]))"
        },
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesout_total{topic=\"pilares\"}[1m]))"
        }
      ]
    },
    {
      "type": "graph",
